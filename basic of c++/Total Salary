#include<iostream>
#include<cmath>
using namespace std;

int main() {
	int basic;
	char grade;
	cin>>basic>>grade;
	double hra= .2 * basic;
	double da=.5*basic;
	double pf=.11*basic;
	int allow=0;
	if(grade=='A')
	allow=1700;
	else if(grade=='B')
	allow=1500;
	else
	allow=1300;

	double total_salery=basic+hra+da+allow-pf;
	int ans=round(total_salery);

cout<<ans;
	
}
